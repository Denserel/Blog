@model AuthUserViewModel

<form asp-controller="Auth" asp-action="Login" method="post">
    <div>
        <label>User</label>
        <input asp-for="UserName" />
    </div>
    <div>
        <label>Password</label>
        <input asp-for="Password" />
    </div>
    <input type="submit" value="Login" />
</form>